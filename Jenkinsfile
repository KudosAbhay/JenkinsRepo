                echo 'python3 hello-world.py'
            }
        }
        stage('Test') 
        {
            steps 
            {
                echo 'Testing phase..'
            }
        }
        stage('Deploy') 
        {
            steps 
            {
                echo 'Deploying phase....'
            }
        }
}
